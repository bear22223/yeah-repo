<h1><%= @user.name %>のタスク一覧</h1>
<div class="row">
    <div class="col-md-10 col-md-offset-1">
        <%= link_to "タスク新規作成" , "#", class: "btn btn-lg btn-primary pull-right"%>
        <table class="table table-hover" id="table-tasks">
            <thead>
                <tr>
                    <th>名称</th>
                    <th>作成日</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <% @list.each do |lista| %>
                <tr>
                    <td id="task-name"><%= link_to lista, "#" %></td>
                    <td><%= "2222/22/22 22:22" %></td>
                    <td><%= link_to "編集", "#", class: "btn btn-success" %> <%=link_to "削除", "#", class: "btn btn-danger" %></td>
                <% end %>
                </tr>
            </tbody>
        </table>
    </div>
</div>